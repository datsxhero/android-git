<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Weather_Apps"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeekWeather"
            android:label="@string/title_activity_week_weather"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TyphoonWarnings"
            android:label="@string/title_activity_typhoon_warnings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EarthquakeWarnings"
            android:label="@string/title_activity_earthquake_warnings"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>